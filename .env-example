# RPC Configuration
RPC_URL=

# ZKSync Configuration
ZKSYNC_VERIFIER_URL=
PRIVATE_KEY=

# Token Configuration
TOKEN_NAME=MyToken
TOKEN_SYMBOL=MTK

# Test Data
# A transaction hash for testing transaction-related methods
TEST_TX_HASH=

# An address for testing account-related methods
TEST_ADDRESS=

# Block and Batch Numbers
# A specific L2 block number for testing block-related methods (instead of 'latest')
TEST_BLOCK_NUMBER=

# A specific L2 block hash for testing block-related methods
TEST_BLOCK_HASH=

# A specific L1 batch number for testing batch-related methods
TEST_L1_BATCH_NUMBER=12345

# Message Index for L2 to L1 proofs
TEST_MESSAGE_INDEX=0

# Message Proof Parameters
# An address that has sent messages from L2 to L1 (required for zks_getL2ToL1MsgProof)
TEST_MESSAGE_PROOF_ADDRESS=

# Debug Tracer Configuration
# Tracer type for debug methods (e.g., 'callTracer', 'prestateTracer')
DEBUG_TRACER_TYPE=callTracer

# Rate Limiting Configuration
# Maximum requests per second (default: 1000)
MAX_REQUESTS_PER_SECOND=1000

# Batch size for requests (default: 10)
BATCH_SIZE=10

# Delay between batches in milliseconds (default: 1000)
BATCH_DELAY_MS=1000
